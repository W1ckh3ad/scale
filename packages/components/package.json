{
  "name": "@scaleds/components-telekom",
  "publishConfig": {
    "access": "restricted"
  },
  "version": "0.1.13",
  "description": "Scale Components Telekom",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/scale-components/scale-components.js",
  "files": [
    "dist/",
    "loader/",
    "hydrate/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "prebuild": "yarn build:theme",
    "build:theme": "rollup -c",
    "build:prod": "stencil build --prod",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e --max-workers=2",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "test:visual.build": "yarn build && yarn monument build",
    "test:visual": "jest --forceExit",
    "format": "prettier --write \"src/**/*.{html,tsx,ts,css,json}\" \"!src/**/*.d.ts\"",
    "lint": "yarn lint:types && yarn lint:style && yarn lint:format",
    "lint:fix": "yarn lint:style --fix",
    "lint:types": "tsc --noEmit",
    "lint:style": "tslint -c tslint.json -p . \"src/**/*.{tsx,ts,css,json}\" \"!src/**/*.d.ts\"",
    "lint:format": "prettier --list-different \"src/**/*.{tsx,ts,css,json}\" \"!src/**/*.d.ts\"",
    "verify": "yarn format && yarn lint:fix",
    "develop": "stencil build --docs --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-syntax-jsx": "7.10.1",
    "@babel/plugin-transform-react-jsx": "7.10.1",
    "@nowseemee/stencil-react-output-target": "0.0.8",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "@rollup/plugin-typescript": "4.0.0",
    "@stencil/core": "1.14.0",
    "@storybook/addon-a11y": "6.0.0-beta.32",
    "@storybook/addon-actions": "6.0.0-beta.32",
    "@storybook/addon-docs": "6.0.0-beta.32",
    "@storybook/addon-knobs": "6.0.0-beta.32",
    "@storybook/addon-links": "6.0.0-beta.32",
    "@storybook/addon-storysource": "6.0.0-beta.32",
    "@storybook/addon-viewport": "6.0.0-beta.32",
    "@storybook/html": "6.0.0-beta.32",
    "@types/classnames": "2.2.8",
    "@types/jest": "24.9.1",
    "@types/lodash": "4.14.149",
    "@types/puppeteer": "1.20.3",
    "date-fns": "^2.6.0",
    "express": "^4.17.1",
    "jest": "^26.0.1",
    "jest-cli": "^24.9.0",
    "jest-image-snapshot": "^4.0.2",
    "jest-puppeteer-docker": "^1.3.2",
    "jss": "^10.0.4",
    "jss-preset-default": "^10.0.4",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "puppeteer": "^4.0.0",
    "rollup": "2.16.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/?(*.)+(.spec).(ts|tsx)"
    ]
  }
}
