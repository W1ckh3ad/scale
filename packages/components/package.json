{
  "name": "@scaleds/components-telekom",
  "publishConfig": {
    "access": "restricted"
  },
  "version": "1.14.0",
  "description": "Scale Components Telekom",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/scale-components/scale-components.js",
  "files": [
    "dist/",
    "loader/",
    "hydrate/"
  ],
  "scripts": {
    "build:theme": "rimraf build && rollup -c",
    "prebuild": "yarn build:theme",
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e --max-workers=2",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "format": "prettier --write \"src/**/*.{html,tsx,ts,css,json}\" \"!src/**/*.d.ts\"",
    "lint": "yarn lint:types && yarn lint:style && yarn lint:format",
    "lint:fix": "yarn lint:style --fix",
    "lint:types": "tsc --noEmit",
    "lint:style": "tslint -c tslint.json -p . \"src/**/*.{tsx,ts,css,json}\" \"!src/**/*.d.ts\"",
    "lint:format": "prettier --list-different \"src/**/*.{tsx,ts,css,json}\" \"!src/**/*.d.ts\"",
    "verify": "yarn format && yarn lint:fix",
    "predevelop": "yarn build:theme",
    "develop": "stencil build --docs --watch",
    "storybook": "start-storybook -p 6006 -s ./build",
    "build-storybook": "build-storybook -s ./build"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-syntax-jsx": "7.10.1",
    "@babel/plugin-transform-react-jsx": "7.10.1",
    "@nowseemee/stencil-react-output-target": "0.0.10",
    "@nowseemee/vue-output-target": "0.0.4",
    "@rollup/plugin-commonjs": "12.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^4.0.0",
    "@stencil/angular-output-target": "^0.0.5",
    "@stencil/core": "^1.16.0",
    "@types/classnames": "2.2.8",
    "@types/jest": "24.9.1",
    "@types/lodash": "4.14.149",
    "@types/puppeteer": "1.20.3",
    "@types/resize-observer-browser": "^0.1.4",
    "babel-loader": "^8.1.0",
    "date-fns": "^2.6.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "jss": "^10.0.4",
    "jss-preset-default": "^10.0.4",
    "jsx-dom": "^6.4.17",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "puppeteer": "^4.0.0",
    "rimraf": "^3.0.2",
    "rollup": "2.16.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "license": "MIT",
  "dependencies": {
    "@proyecto26/animatable-component": "^1.1.8",
    "@scaleds/icons-telekom": "^1.14.0",
    "classnames": "^2.2.6",
    "web-animations-js": "^2.3.2"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/?(*.)+(.spec).(ts|tsx)"
    ]
  }
}
