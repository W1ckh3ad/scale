<script src="/symbol_names.js"></script>
<script>
    Array.from(document.querySelectorAll("scale-tag")).forEach((btn,i) => {
        btn.dataset.sketchSymbol = `Tags / ${
                order(1, getCategoryName(btn))
            } / ${
                order(2, getSizeName(btn))
            } / ${
                order(3, btn.getAttribute('variant') ? 'Black' : 'Colored')
            }${
                getCategoryName(btn) === 'Static'
                ? ''
                : ' / ' + order(4, getStateName(btn))
            }`;
    });
</script>