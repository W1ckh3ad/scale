<script src="/symbol_names.js"></script>
<script>
    Array.from(document.querySelectorAll("scale-input")).forEach((btn,i) => {
        btn.dataset.sketchSymbol = `Select Box / ${
                order(0, btn.hasAttribute('placeholder') ? 'Input' : 'Dropdown')
            } / ${
                order(1, getSizeName(btn))
            } / ${
                order(2, btn.hasAttribute('helper-text') ? 'Info On' : 'Info Off')
            } / ${
                order(3, btn.getAttribute('status') === 'error' ? 'Error On' : 'Error Off')
            } / ${
                order(4, btn.value ? 'Has Selection' : 'Has No Selection')
            } / ${
                order(6, getStateName(btn))
            }`;
    });
</script>