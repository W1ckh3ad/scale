<style>
    scale-list-item {
        display: list-item;
    }
    scale-list-item[type="decimal"] {
        list-style-type: decimal;
    }
</style>
<div data-sketch-artboard="Text Lists" class="artboard" data-sketch-unnamed-component-path="Text Lists">
    <h1 class="title--artboard">Text Lists</h1>

    <h2 class="title--artboard-section">Ordered Lists</h2>
    <div class="agent-states--grid">
        <div class="agent-states--item">
            <p class="agent-states--label">:available</p>
            <scale-list data-sketch-key="text-lists-1">
              <scale-list-item type="decimal">List entry with some text</scale-list-item>
              <scale-list-item type="decimal">List entry with some text</scale-list-item>
              <scale-list-item type="decimal">List entry with some text</scale-list-item>
            </scale-list>
        </div>
        <div class="agent-states--item">
            <p class="agent-states--label">:hover</p>
        </div>
        <div class="agent-states--item">
            <p class="agent-states--label">:focus</p>
        </div>
        <div class="agent-states--item">
            <p class="agent-states--label">:pressed</p>
        </div>
        <div class="agent-states--item">
            <p class="agent-states--label">:disabled</p>
        </div>
    </div>

    <h2 class="title--artboard-section">Unordered Lists</h2>
    <div class="agent-states--grid">
        <div class="agent-states--item">
            <p class="agent-states--label">:available</p>
            <scale-list data-sketch-key="text-lists-2" variant="unordered">
              <scale-list-item>List entry with some text</scale-list-item>
              <scale-list-item>List entry with some text</scale-list-item>
              <scale-list-item>List entry with some text</scale-list-item>
            </scale-list>
        </div>
        <div class="agent-states--item">
            <p class="agent-states--label">:hover</p>
        </div>
        <div class="agent-states--item">
            <p class="agent-states--label">:focus</p>
        </div>
        <div class="agent-states--item">
            <p class="agent-states--label">:pressed</p>
        </div>
        <div class="agent-states--item">
            <p class="agent-states--label">:disabled</p>
        </div>
    </div>

</div>

<script src="/symbol_names.js"></script>
<script>
    Array.from(document.querySelectorAll("scale-list")).forEach(btn => {
        btn.dataset.sketchSymbol = `Text Lists / ${
                order(0, btn.getAttribute('variant') === 'unordered' ? 'Unordered Lists' : 'Ordered Lists')
            }`;
    });
    Array.from(document.querySelectorAll("scale-list-item")).forEach(btn => {
        btn.dataset.sketchSymbol = `Text Lists / List Item / ${
                order(1, btn.getAttribute('type') === 'decimal' ? 'Decimal' : 'Default')
            }`;
    });
</script>