<script src="/symbol_names.js"></script>
<script>
    Array.from(document.querySelectorAll("scale-slider")).forEach(btn => {
        btn.dataset.sketchSymbol = `Slider / ${
                order(0, getCategoryName(btn))
            } / ${
                order(2, btn.hasAttribute('label') ? 'Label On' : 'Label Off')
            } / ${
                order(1, btn.hasAttribute('show-value') ? 'Counter Off' : 'Counter On')
            } / ${
                order(4, btn.hasAttribute('disabled') ? 'Disabled' : (btn.hasAttribute('custom-color') ? 'Custom Color' : 'Normal'))
            }`;
    });
</script>